{
  "functions": [
    {
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug..log"
      ],
      "source": ".output/server"
    }
  ],
  "hosting": {
    "cleanUrls": true,
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "public": ".output/public",
    "rewrites": [
      {
        "function": "server",
        "source": "**"
      }
    ]
  }
}
